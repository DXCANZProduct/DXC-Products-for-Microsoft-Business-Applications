{
  "build": {
    "content": [
      {
        "files": [
          "**/*.md",
          "**/**.yml"
        ],
        "exclude": [
          "**/obj/**",
          "**/includes/**",
          "README.md",
          "LICENSE",
          "LICENSE-CODE",
          "ThirdPartyNotices",
          "PULL_REQUEST_TEMPLATE.md",
          "ISSUE_TEMPLATE.md",
          "**/_themes/**",
          "**/_themes.pdf/**"
        ]
      }
    ],
    "resource": [
      {
        "files": [
          "**/**.png",
          "**/**.jpg",
          "**/**.jpeg",
          "**/**.gif",
          "**/**.svg"
        ],
        "exclude": [
          "**/obj/**",
          "**/includes/**"
        ]
      }
    ],
    "overwrite": [],
    "externalReference": [],
    "globalMetadata": {
      "feedback_system": "GitHub",
      "feedback_github_repo": "DXCANZProduct/DXC-Products-for-Microsoft-Business-Applications",
      "feedback_product_url": "https://dxcanzproduct.github.io/DXC-Products-for-Microsoft-Business-Applications",
      "layout": "Conceptual",
      "searchScope": [
        "dynamics365-finops",
        "Dynamics 365",
        "Unified Operations"
      ],
      "uhfHeaderId": "MSDocsHeader-Dynamics365",
      "brand": "dyn-ops",
      "_op_documentIdPathDepotMapping": {
        "FINU/": {
          "folder_relative_path_in_docset": ".",
          "depot_name": "MSDN.d365OpsFINU"
        }
      },
      "recommendations": "true",
      "_noindex": true,
      "_norobots": true
    },
    "fileMetadata": {
      "search.app": {
        "FINU/**/*.md": [
          "FinanceAndOperations"
        ]
      },
      "feedback_product_url": {
        "retail/**/*.md": "https://ideas.dynamics.com"
      },
      "searchScope": {
        "FINU/**.md": [
          "dynamics365-talent",
          "Dynamics 365",
          "Unified Operations",
          "Talent"
        ],
        "DOCUSIGN/**.md": [
          "dynamics365-talent",
          "Dynamics 365",
          "Unified Operations",
          "Talent"
        ]
      },
      "ms.service": {
        "FINU/**/**.*": "Finance Utilities",
        "DOCUSIGN/**/**.*": "DocuSign"
      },
      "ms.search.scope": {
        "FINU/**/**.md": "Core, Operations, Retail",
        "DOCUSIGN/**/**.md": "Core, Operations, Retail"
      }
    },
    "template": [
      "mstemplate"
    ],
    "dest": "_site",
    "markdownEngineName": "markdig"
  }
}
